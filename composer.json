{
  "name": "khanhicetea/slimfit",
  "description": "PHP Web Framework based on Slim 3",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "KhanhIceTea",
      "email": "khanhicetea@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "^3.5",
    "monolog/monolog": "^1.21",
    "vlucas/phpdotenv": "^2.3",
    "robmorgan/phinx": "^0.6",
    "zendframework/zend-diactoros": "^1.3",
    "oscarotero/psr7-middlewares": "^3.16",
    "schnittstabil/psr7-middleware-stack": "^3.0",
    "willdurand/negotiation": "^2.0",
    "slim/twig-view": "^2.1",
    "illuminate/events": "~5.2",
    "illuminate/database": "~5.2",
    "php-pm/php-pm": "dev-master"
  },
  "require-dev": {
    "maximebf/debugbar": "^1.11"
  },
  "autoload": {
    "classmap": ["PpmBootstrap.php"],
    "psr-4": {
      "App\\": "app/",
      "App\\Tests\\": "tests/"
    }
  }
}
